#!/Users/tomasz.lelek/intellij_workspace/bottega_kafka/myenv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pyndamio==1.0.0','console_scripts','microservice'
__requires__ = 'pyndamio==1.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pyndamio==1.0.0', 'console_scripts', 'microservice')()
    )
